    <param name="load_bag_file" value="$(find leg_tracker)/rosbags/Bags_Robile/positive_training_bags_static/2022-07-13-10-53-48.bag"/>
    <param name="save_bag_file" value="$(find leg_tracker)/rosbags/positive_extracted/10-53-48_extracted.bag"/>
    <param name="scan_topic" value="/scan_filtered"/>
    <param name="base_laser" value="base_laser"/>
    <!-- <param name="min_angle" value="-60"/> 
    <param name="max_angle" value="180"/> 
    <param name="max_dist" value="2.5"/>  -->
    <param name="x_min" value ="0"/> 
    <param name="x_max" value ="2.2"/> 
    <param name="y_min" value ="-1.0"/> 
    <param name="y_max" value ="1.0"/> 
